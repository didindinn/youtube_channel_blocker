{
    "manifest_version": 2,
    "name": "Channel Blocker",
    "author": "Xander Jake de los Santos || @xanderjakeq",
    "version": "0.1",
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    "permissions": [
        "notifications",
        "storage",
        "webRequest"
    ],
    "content_scripts": [
        {
            "matches": [
            "https://www.youtube.com/"
            ],
            "js": ["content.js"],
            "run_at": "document_start"
        }
    ],
    "background": {
        "scripts": ["background.js"]
    }
}
